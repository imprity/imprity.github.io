; https://editorconfig.org/

root = true

[*]
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
indent_style = space
indent_size = 4
end_of_line = lf

[{Makefile,go.mod,go.sum,*.go,.gitmodules,*.bat}]
indent_style = tab
indent_size = 4

[*.bat]
end_of_line = crlf

[*.md]
indent_size = 4
trim_trailing_whitespace = false

eclint_indent_style = unset
